# dapscoind01
alias start01='dapscoind01 -datadir=/root/.dapscoin01 -wallet=wallet01.dat'
alias reindex01='dapscoind01 -datadir=/root/.dapscoin01 -wallet=wallet01.dat - reindex'
alias dapscoind01='dapscoind01 -rpcuser=DAPSMN01 -rpcpassword=DAPSMN010 -rpcport=53574'
alias getinfo01='dapscoin-cli01 -rpcuser=DAPSMN01 -rpcpassword=DAPSMN010 -rpcport=53574 getinfo'
alias stop01='dapscoin-cli01 -rpcuser=DAPSMN01 -rpcpassword=DAPSMN010 -rpcport=53574 stop'
alias mnstatus01='dapscoin-cli01 -rpcuser=DAPSMN01 -rpcpassword=DAPSMN010 -rpcport=53574 masternode status'
